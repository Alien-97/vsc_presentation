{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "start",
      "problemMatcher": []
    },

    {
      "version": "0.1.0",
      "command": "$PYTHON_PATH",
      "args": ["app.py"],
      "problemMatcher": {
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern": {
          "regexp": "^(.*)+s$",
          "message": 1
        }
      }
    }
  ]
}
